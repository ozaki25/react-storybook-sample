{
  "core": {
    "workingDir": ".reg",
    "actualDir": "__screenshots__",
    "thresholdRate": 0,
    "addIgnore": true,
    "ximgdiff": {
      "invocationType": "client"
    }
  },
  "plugins": {
    "reg-keygen-git-hash-plugin": {},
    "reg-notify-github-plugin": {
      "clientId": "$GITHUB_CLIENT_ID"
    },
    "reg-publish-s3-plugin": {
      "bucketName": "$S3_BUCKET_NAME",
      "pathPrefix": "$S3_PATH_PREFIX"
    }
  }
}
